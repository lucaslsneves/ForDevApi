{
  "compilerOptions": {
    "target": "es2019", 
    "module": "commonjs",
    "lib": [
      "es6"
    ], 
    "allowJs": true, 
    "outDir": "./dist", 
    "rootDir": "./src", 
    "removeComments": true, 
    "strict": true,
    "esModuleInterop": true, 
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true
  }
}